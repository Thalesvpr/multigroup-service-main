{
    "version": 2,
    "builds": [
      {
        "src": "apps/multigroup-service/dist/main.js",
        "use": "@vercel/node",
        "config": {
          "regions": ["sfo1"]
        }
      },
      {
        "src": "apps/auth/dist/main.js",
        "use": "@vercel/node",
        "config": {
          "regions": ["sfo1"]
        }
      },
      {
        "src": "apps/user/dist/main.js",
        "use": "@vercel/node",
        "config": {
          "regions": ["sfo1"]
        }
      }
    ],
    "routes": [
      {
        "src": "/multigroup-service/(.*)",
        "dest": "/apps/multigroup-service/dist/main.js"
      },
      {
        "src": "/auth/(.*)",
        "dest": "/apps/auth/dist/main.js"
      },
      {
        "src": "/user/(.*)",
        "dest": "/apps/user/dist/main.js"
      }
    ]
  }
  